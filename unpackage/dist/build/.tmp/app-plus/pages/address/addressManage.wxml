<view class="content" bind:__l="__l"><view class="row b-b"><text class="tit">联系人</text><input class="input" type="text" placeholder="收货人姓名" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['$0','consignee','$event',[]],['addressData']]]]]}}" value="{{addressData.consignee}}" bindinput="__e"/></view><view class="row b-b"><text class="tit">手机号</text><input class="input" type="number" placeholder="收货人手机号码" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['$0','mobile','$event',[]],['addressData']]]]]}}" value="{{addressData.mobile}}" bindinput="__e"/></view><view data-event-opts="{{[['tap',[['showMulLinkageThreePicker',['$event']]]]]}}" class="row b-b" bindtap="__e"><text class="tit">地址</text><text class="input">{{''+addressData.pickerText+''}}</text><text class="yticon icon-shouhuodizhi"></text></view><view class="row b-b"><text class="tit">门牌号</text><input class="input" type="text" placeholder="楼号、门牌" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['$0','address','$event',[]],['addressData']]]]]}}" value="{{addressData.address}}" bindinput="__e"/></view><view class="row default-row"><text class="tit">设为默认</text><switch checked="{{addressData.defaule}}" color="#4fbeed" data-event-opts="{{[['change',[['switchChange',['$event']]]]]}}" bindchange="__e"></switch></view><button data-event-opts="{{[['tap',[['confirm',['$event']]]]]}}" class="add-btn" bindtap="__e">提交</button><block wx:if="{{deletetwo==1}}"><button data-event-opts="{{[['tap',[['deleteOne',['$event']]]]]}}" class="add-btn" bindtap="__e">删除地址</button></block><mpvue-city-picker class="vue-ref" themeColor="{{themeColor}}" pickerValueDefault="{{cityPickerValueDefault}}" data-ref="mpvueCityPicker" data-event-opts="{{[['^onCancel',[['onCancel']]],['^onConfirm',[['onConfirm']]]]}}" bind:onCancel="__e" bind:onConfirm="__e"></mpvue-city-picker></view>